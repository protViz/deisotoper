Package: deisotoper
Type: Package
Title: Detection of Isotope Pattern of a Mass Spectrometric Measurement
Version: 0.0.7
Authors@R: c(person("Christian", "Panse", email = "cp@fgcz.ethz.ch",
	role = c("cre", "aut"), comment = c(ORCID = "0000-0003-1975-3064")),
